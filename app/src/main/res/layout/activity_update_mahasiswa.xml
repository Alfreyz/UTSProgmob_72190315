<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CRUD.UpdateMahasiswaActivity">
    <EditText
        android:id="@+id/edId"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:clickable="false"
        android:editable="false"
        android:ems="10"
        android:enabled="false"
        android:outlineProvider="none"
        android:text="ID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edKode"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginEnd="48dp"
        android:outlineProvider="none"
        android:clickable="false"
        android:editable="false"
        android:ems="10"
        android:enabled="false"
        android:text="Kode"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edId" />

    <EditText
        android:id="@+id/edNama"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="none"
        android:text="Nama"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edKode" />

    <EditText
        android:id="@+id/edHari"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="background"
        android:text="Hari"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edNama" />

    <EditText
        android:id="@+id/edSesi"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="none"
        android:text="Sesi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edHari" />

    <EditText
        android:id="@+id/edSks"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="none"
        android:text="Sks"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edSesi" />

    <EditText
        android:id="@+id/edNim_progmob"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="none"
        android:text="NIM"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edSks" />

    <EditText
        android:id="@+id/edKode_cari"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="48dp"
        android:minHeight="48dp"
        android:outlineProvider="none"
        android:text="Kode Cari"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edNim_progmob"
        android:clickable="false"
        android:editable="false"
        android:ems="10"
        android:enabled="false"/>

    <Button
        android:id="@+id/btnSimpanMahasiswa"
        android:layout_width="408dp"
        android:layout_height="48dp"
        android:text="Simpan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edKode_cari" />

</androidx.constraintlayout.widget.ConstraintLayout>